{
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "es6": true,
        "browser": true,
        "jquery": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off"
    },
    "globals": {
        "toAbsoluteURL": true
    }
}
